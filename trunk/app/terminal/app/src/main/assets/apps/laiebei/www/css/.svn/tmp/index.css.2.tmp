/*******keybord begin***********/
.keybord-wrap{
	width: 400px;
	height: 400px;
	background: rgb(215,224,231);
	color:rgb(153,157,161);
	font-size: 22px;
    position: absolute;
    left: 50%;
    margin-left: -200px;
    top: 50%;
    margin-top: -200px;
}
}
.keybord-wrap dl{
	margin:0;
}
dl.key>dd{
	width: 50px;
	height: 50px;
	line-height: 50px;
	text-align: center;
	margin:10px 10px 0 0;
	background: rgb(234,239,242);
	border-radius: 5px;
	display: block;
	float: left;
	cursor: pointer;
}
dl.key>dd:last-child{
	width:170px
}
dl.oper>dd{
	width: 80px;
	height: 50px;
	line-height: 50px;
	text-align: center;
	background: rgb(189,194,197);
	border-radius: 5px;
	display: block;
	margin: 10px 0 0 0;
	cursor: pointer;
}
dl.oper>dd:last-child{
	height: 110px;
	line-height: 110px;
}
.keybord-wrap .close-oper{
	width: 50px;
	height: 50px;
	border-radius: 0 0 0 50px;
	position: absolute;
	top:0;
	right: 0;
	
}
.keybord-wrap dd:active,.keybord-wrap dd:hover{
	box-shadow: inset 0 2px 10px #ccc;
}
