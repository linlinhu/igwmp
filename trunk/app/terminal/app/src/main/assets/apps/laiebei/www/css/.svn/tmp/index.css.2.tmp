@-webkit-keyframes ball-scale {
  0% {
    -webkit-transform: scale(0);
            transform: scale(0); }

  100% {
    -webkit-transform: scale(1);
            transform: scale(1);
    opacity: 0; } }

@keyframes ball-scale {
  0% {
    -webkit-transform: scale(0);
            transform: scale(0); }

  100% {
    -webkit-transform: scale(1);
            transform: scale(1);
    opacity: 0; 
    } }

.ball-scale{
  background: skyblue;
  border-radius: 100%;
  -webkit-animation-fill-mode: both;
          animation-fill-mode: both;
  display: inline-block;
  -webkit-animation: ball-scale 0.5s 0s ease-in-out infinite;
          animation: ball-scale 0.5s 0s ease-in-out infinite; }
.item{
	display: inline-block;
	padding:10px 20px;
	border:1px solid;
}
#loader_animate{
	position: absolute;
	display: none;
	overflow: hidden;
}
.ball-scale{
	position: absolute;
}
/*****loader_animate end*******/